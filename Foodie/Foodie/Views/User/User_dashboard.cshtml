@model IEnumerable<Foodie.Models.CategoryModel>
@{
    ViewData["Title"] = "User_dashboard";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<!-- offer section -->
<section class="offer_section layout_padding-bottom">
    <div class="offer_container">
        <div class="container">
            <div class="row">
                <!-- Start Repeater Item Template -->
                @foreach(var item in Model)
                {
                    <div class="col-md-6">
                            <div class="box">
                                <div class="img-box">
                                    <a href="#">
                                        ">
                                        <img src="~/@item.ImageUrl" alt="">
                                    </a>
                                </div>
                                <div class="detail-box">
                                    <h5>@item.Name</h5>
                                    <h6><span>20%</span> Off</h6>
                                    <a href="#">
                                        Order Now
                                        <svg version="1.1" id="Capa_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
                                             x="0px" y="0px" viewBox="0 0 456.029 456.029" style="enable-background:new 0 0 456.029 456.029;"
                                             xml:space="preserve">
                          <!-- SVG paths here -->
                                    </svg>
                                    </a>
                                </div>
                            </div>
                    
                    </div>
                }
                <!-- End Repeater Item Template -->
            </div>
        </div>
    </div>
</section>
<!-- end offer section -->
